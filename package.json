{
  "name": "antlr-odata",
  "version": "0.0.1",
  "description": "Antlr 4 OData Typescript parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -t ES6 -p tsconfig.json",
    "test": "npm run-script odata-lite-antlr & ts-mocha --paths -t ES6 -p test/tsconfig.json test/**/*.spec.ts -w --watch-extensions ts",
    "odata-lite-antlr": "antlr4ts -visitor -listener src/grammar/OData4Lite.g4 -o src"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "odata",
    "odata4",
    "parser",
    "antlr",
    "antlr4",
    "typescript"
  ],
  "author": "Cameron Maunder",
  "license": "MIT",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3"
  },
  "devDependencies": {
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "@types/assert": "^1.4.3",
    "@types/mocha": "^5.2.7",
    "assert": "^2.0.0",
    "mocha": "^6.2.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "rimraf": "^3.0.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  }
}
